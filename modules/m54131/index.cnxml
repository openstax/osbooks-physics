     <row>
      <entry>normal force for a nonaccelerating horizontal surface</entry>
      <entry> <m:math>
       <m:mrow>
        <m:mstyle mathvariant="bold" mathsize="normal"><m:mi>N</m:mi></m:mstyle><m:mo>=</m:mo><m:mi>m</m:mi><m:mstyle mathvariant="bold" mathsize="normal"><m:mi>g</m:mi></m:mstyle>
       </m:mrow>
      </m:math> </entry>
     </row>
     <row>
