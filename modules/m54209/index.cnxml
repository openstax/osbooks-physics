                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mtext>+</m:mtext><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mtext>0</m:mtext><m:mtext>.500 kg</m:mtext>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mo>−</m:mo><m:mtext>0</m:mtext><m:mtext>.500 m/s</m:mtext>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>
                      <m:mrow>
                        <m:mtext>0</m:mtext><m:mtext>.350</m:mtext><m:mtext>kg</m:mtext>
                      </m:mrow>
                      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
