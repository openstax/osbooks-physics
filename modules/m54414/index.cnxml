        </m:msub>
        </m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>k</m:mi><m:msub>
         <m:mi>q</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>q</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:mo>/</m:mo><m:msubsup>
         <m:mi>r</m:mi>
