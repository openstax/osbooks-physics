    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>20</m:mn><m:mspace width="0.25em"/><m:mo>×</m:mo><m:mspace width="0.25em"/><m:msup>
         <m:mrow>
          <m:mn>10</m:mn></m:mrow>
         <m:mrow>
         <m:mn>−6</m:mn></m:mrow>
        </m:msup>
        <m:mtext>N</m:mtext></m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mn>0.050</m:mn><m:mspace width="0.3em"/><m:mtext>m</m:mtext></m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
         <m:mn>2</m:mn>
        </m:msup>
        </m:mrow>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mn>0.030</m:mn><m:mspace width="0.3em"/><m:mtext>m</m:mtext></m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
