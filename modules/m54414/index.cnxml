        </m:msubsup>
        </m:mrow>
       <m:mrow>
        <m:mi>k</m:mi><m:msub>
         <m:mi>q</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>q</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:mo>/</m:mo><m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>i</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
      </m:mfrac>
       <m:mtext>.</m:mtext>
      </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>i</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>f</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mrow>
      <m:msub>
       <m:mi>F</m:mi>
       <m:mtext>f</m:mtext>
      </m:msub>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msub>
       <m:mi>F</m:mi>
       <m:mtext>i</m:mtext>
      </m:msub>
      <m:mfrac>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>i</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>f</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
      </m:mfrac>
<m:mtext>.</m:mtext>
      </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>

</m:mrow>
       </m:math> </equation><para id="fs-id1167062819999">Inserting the known quantities yields</para>
<equation id="fs-id1164565546700"> <m:math display="block">
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:msub>
       <m:mi>F</m:mi>
       <m:mtext>f</m:mtext>
      </m:msub>
      </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:msub>
       <m:mi>F</m:mi>
       <m:mtext>i</m:mtext>
      </m:msub>
      <m:mfrac>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>i</m:mtext>
         <m:mn>2</m:mn>
        </m:msubsup>
        </m:mrow>
       <m:mrow>
        <m:msubsup>
         <m:mi>r</m:mi>
         <m:mtext>f</m:mtext>
         <m:mn>2</m:mn>
