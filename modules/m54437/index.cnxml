    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mn>−16.0</m:mn><m:mspace width="0.3em"/><m:mtext>C</m:mtext>
       </m:mrow>
       <m:mrow>
        <m:mn>2</m:mn><m:mspace width="0.25em"/><m:mo>×</m:mo><m:mspace width="0.25em"/><m:msup>
