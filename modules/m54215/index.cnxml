<item> <m:math>
 
  <m:mrow>
   <m:mi>Δ</m:mi><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>p</m:mtext></m:mstyle><m:mo>=</m:mo><m:mtext>Δ</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>m</m:mi><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>v</m:mtext></m:mstyle><m:mo stretchy="false">)</m:mo>
  </m:mrow>
   
</m:math> </item>
<item> <m:math>
 
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>p</m:mtext></m:mstyle>
    </m:mrow>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mi>t</m:mi>
    </m:mrow>
   </m:mfrac>
   <m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>m</m:mi><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>v</m:mtext></m:mstyle><m:mo stretchy="false">)</m:mo>
    </m:mrow>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mi>t</m:mi>
    </m:mrow>
   </m:mfrac>

  </m:mrow>
 
</m:math> </item>
<item> <m:math>
 
  <m:mrow>
   <m:mtext>Δ</m:mtext><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>p</m:mtext></m:mstyle><m:mo>=</m:mo><m:mtext>Δ</m:mtext><m:mo stretchy="false">(</m:mo><m:mfrac>
    <m:mi>m</m:mi>
    <m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>v</m:mtext></m:mstyle>
   </m:mfrac>
   <m:mo stretchy="false">)</m:mo>
  </m:mrow>
 
</m:math> </item>
<item> <m:math>
 
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>p</m:mtext></m:mstyle>
    </m:mrow>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mi>t</m:mi>
    </m:mrow>
