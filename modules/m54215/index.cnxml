    <m:mn>1</m:mn>
    <m:mrow>
     <m:mtext>Δ</m:mtext><m:mi>t</m:mi>
    </m:mrow>
   </m:mfrac>
   <m:mtext>·</m:mtext><m:mtext>Δ</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>m</m:mi><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>v</m:mtext></m:mstyle><m:mo stretchy="false">)</m:mo>
  </m:mrow>
 
</m:math> </item>
</list>

