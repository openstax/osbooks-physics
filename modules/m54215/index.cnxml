    <m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>F</m:mtext></m:mstyle>
    <m:mrow>
     <m:mtext>net</m:mtext>
    </m:mrow>
   </m:msub>
   <m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mi>m</m:mi><m:mo>Δ</m:mo><m:mi>t</m:mi>
    </m:mrow>
    <m:mrow>
     <m:mo>Δ</m:mo><m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>v</m:mtext></m:mstyle>
    </m:mrow>
   </m:mfrac>

  </m:mrow>
   
</m:math> </item>
<item> <m:math>
 
  <m:mrow>
   <m:msub>
    <m:mstyle mathvariant="bold" mathsize="normal"><m:mtext>F</m:mtext></m:mstyle>
    <m:mrow>
     <m:mtext>net</m:mtext>
